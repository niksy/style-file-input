{
  "name": "style-file-input",
  "version": "2.1.0",
  "description": "Style file input element.",
  "license": "MIT",
  "author": "Ivan NikoliÄ‡ <niksy5@gmail.com> (http://ivannikolic.com)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "style": "./dist/index.css",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "style": "dist/index.css",
  "directories": {
    "test": "test"
  },
  "files": [
    "dist/",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.js && cp index.css dist/index.css",
    "lint": "eslint '{index,lib/**/*,test/**/*}.js' && stylelint 'index.css'",
    "lint:types": "tsc",
    "prepare": "if [[ -x \"$(command -v husky)\" ]]; then husky; fi",
    "prepublishOnly": "npm run build",
    "postpublish": "GITHUB_TOKEN=$GITHUB_RELEASE_TOKEN github-release-from-changelog",
    "prerelease": "npm run lint && npm run lint:types && npm run build",
    "release": "np --no-release-draft",
    "test": "npm run test:automated",
    "test:automated": "BABEL_ENV=test karma start",
    "test:automated:watch": "npm run test:automated -- --auto-watch --no-single-run",
    "test:generate-static-site": "BABEL_ENV=test rollup --config ./test/manual/rollup.config.js",
    "test:generate-static-site:watch": "npm run test:generate-static-site -- --watch",
    "test:manual": "npm run test:generate-static-site:watch",
    "version": "if [ $(git rev-parse --abbrev-ref HEAD) == 'master' ]; then sed -i '' '/\\[unreleased\\]:/d' CHANGELOG.md && version-changelog CHANGELOG.md && changelog-verify CHANGELOG.md && git add CHANGELOG.md; else echo; fi"
  },
  "dependencies": {
    "classlist-multiple-values": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@istanbuljs/esm-loader-hook": "^0.2.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@types/karma-fixture": "^0.2.8",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.24",
    "changelog-verify": "^1.1.2",
    "core-js": "^3.46.0",
    "cpy": "^11.1.0",
    "del": "^7.1.0",
    "eslint": "^9.39.1",
    "eslint-config-nitpick": "^14.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "execa": "^8.0.1",
    "github-release-from-changelog": "^2.1.1",
    "globals": "^16.5.0",
    "globby": "^14.1.0",
    "husky": "^9.1.7",
    "karma": "^6.4.4",
    "karma-browserstack-launcher": "^1.6.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "7.0.7",
    "karma-sourcemap-loader": "^0.3.8",
    "lint-staged": "^16.2.6",
    "minimist": "^1.2.8",
    "mocha": "^10.8.2",
    "node-stdlib-browser": "^1.3.1",
    "np": "^10.2.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.6.2",
    "rollup": "^4.52.5",
    "rollup-plugin-istanbul": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-static-site": "0.1.0",
    "simulant": "^0.2.2",
    "stylelint": "^16.25.0",
    "stylelint-config-nitpick": "^11.1.0",
    "stylelint-prettier": "^5.0.3",
    "suitcss-components-test": "^1.0.0",
    "typescript": "^5.9.3",
    "version-changelog": "^3.1.1"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "file",
    "fileinput",
    "input",
    "style"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niksy/style-file-input.git"
  },
  "bugs": {
    "url": "https://github.com/niksy/style-file-input/issues"
  },
  "homepage": "https://github.com/niksy/style-file-input#readme"
}
